
@{
    ViewBag.Title = "RecentCom";
    Layout = "~/Views/Developer/DeveloperIndex.cshtml";
}

<div class="container mt-5">
    <div class="row">
        <div class="col-lg-8 mx-auto">
            <h1 class="text-center mb-4">Recent Commits</h1>

            <!-- Commit list -->
            <ul class="list-group">
                <li class="list-group-item">
                    <div class="d-flex justify-content-between">
                        <div>
                            <h5 class="mb-1">Commit Title</h5>
                            <p class="mb-1">Commit message or description</p>
                            <small>Author Name - Date</small>
                        </div>
                        <div>
                            <a href="#" class="btn btn-sm btn-outline-primary mr-2"><i class="fas fa-eye"></i> View</a>
                            <a href="#" class="btn btn-sm btn-outline-danger"><i class="fas fa-trash"></i> Delete</a>
                        </div>
                    </div>
                </li>
                <!-- Add more <li> items for additional commits -->
            </ul>

            <!-- Pagination -->
            <nav aria-label="Page navigation">
                <ul class="pagination justify-content-center mt-4">
                    <li class="page-item disabled">
                        <a class="page-link" href="#" tabindex="-1" aria-disabled="true">Previous</a>
                    </li>
                    <li class="page-item"><a class="page-link" href="#">1</a></li>
                    <li class="page-item"><a class="page-link" href="#">2</a></li>
                    <li class="page-item"><a class="page-link" href="#">3</a></li>
                    <li class="page-item">
                        <a class="page-link" href="#">Next</a>
                    </li>
                </ul>
            </nav>
        </div>
    </div>
</div>
