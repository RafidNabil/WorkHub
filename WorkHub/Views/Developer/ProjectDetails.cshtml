
@{
    ViewBag.Title = "ProjectDetails";
    Layout = "~/Views/Developer/DeveloperIndex.cshtml";

}

<div class="container mt-5">
    <div class="row">
        <div class="col-lg-8">
            <!-- Main content area -->
            <h1>Project Title</h1>
            <p class="lead">Project description or summary</p>
            <p>Details about the project...</p>

            <!-- Example: Project image -->
            <img src="project-image.png" class="img-fluid mb-3" alt="Project Image">

            <!-- Example: Project details list -->
            <h3>Project Details</h3>
            <ul>
                <li><strong>Category:</strong> Category name</li>
                <li><strong>Client:</strong> Client name</li>
                <li><strong>Date:</strong> Date completed</li>
                <!-- Add more details as needed -->
            </ul>

            <!-- Example: Project features section -->
            <h3>Key Features</h3>
            <ul>
                <li>Feature 1</li>
                <li>Feature 2</li>
                <li>Feature 3</li>
                <!-- Add more features as needed -->
            </ul>

            <!-- Example: Call to action buttons -->
            <div class="mt-4">
                <a href="#" class="btn btn-primary mr-2">Visit Website</a>
                <a href="#" class="btn btn-secondary">View Code</a>
            </div>
        </div>

        <!-- Sidebar for additional content (optional) -->
        <div class="col-lg-4">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Project Information</h5>
                    <p class="card-text">Additional details or related links</p>
                </div>
            </div>
        </div>
    </div>
</div>
